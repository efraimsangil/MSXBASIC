10 SCREEN 2,1
20 SE=1:X=128:Y=96:P=0
30 FOR F=1 TO 4
40 FOR T=1 TO 8
50 READ A$
60 S$=S$+CHR$(VAL("&H"+A$))
70 NEXT T
80 SPRITE$(F)=S$:S$=""
90 NEXT F
100 IF X MOD 4 =0 THEN P=(P+1) MOD 2
110 IF SE=1 THEN S=P+1 ELSE S=P+3
120 PUT SPRITE 0,(X,96),15,S
130 X=X+SE
140 IF X<0 OR X>240 THEN SE=-SE
150 GOTO 100
