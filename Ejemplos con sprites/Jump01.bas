10 'JUMP01.BAS
30 SCREEN 2
31 OPEN "GRP:" AS #1
40 SPRITE$(1)=STRING$(8,CHR$(255))
50 DEFINT X,Y
60 X=100:Y=170:YF=50:FL=0
70 S=STICK(0)
71 DRAW "BM100,100"
72 PRINT#1,"Y";Y
80 IF S=1 THEN GOSUB 200 
160 PUT SPRITE 0, (X,Y), 2, 1
180 GOTO 70 'Movimiento del sprite del jugador
200 ' Salto del sprite
210 IF Y>YF AND FL=0 THEN Y=Y-1:PUT SPRITE 0, (X,Y), 2, 1
212 IF Y=50 AND FL=0 THEN FL=1
213 IF Y<169 AND FL=1 THEN Y=Y+1:PUT SPRITE 0, (X,Y), 2, 1
214 IF Y=170 AND FL=1 THEN FL=0:Y=170:RETURN
214 GOTO 200